-- CREATING DATABASE
CREATE DATABASE MY_DATABASE;

-- USING DATABASE
USE MY_DATABASE;

-- CREATING TABLE
create table users(
ID INT auto_increment primary KEY,
NAME VARCHAR(100) NOT NULL,
EMAIL VARCHAR(100) UNIQUE NOT NULL,
GENDER ENUM('MALE', 'FEMALE', 'OTHER'),
DATE_OF_BIRTH DATE,
CREATED_AT TIMESTAMP DEFAULT current_timestamp
);
